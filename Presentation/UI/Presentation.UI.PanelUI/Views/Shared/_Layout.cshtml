<!doctype html>
<html lang="en">
<head>
    <title> Boostionic - Home</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Boostionic Bootstrap 4.5.0 Admin Template">
    <meta name="author" content="WrapTheme, design by: ThemeMakker.com">
    <link rel="icon" href="~/boostionic/html/docs/favicon.ico" type="image/x-icon">

    <!-- VENDOR CSS -->
    <link rel="stylesheet" href="~/boostionic/html/dist/assets/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/boostionic/html/dist/assets/vendor/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/boostionic/html/dist/assets/vendor/toastr/toastr.min.css">
    <link rel="stylesheet" href="~/boostionic/html/dist/assets/vendor/charts-c3/plugin.css" />

    <!-- MAIN Project CSS file -->
    <link rel="stylesheet" href="~/boostionic/html/dist/assets/css/main.css">
    @await RenderSectionAsync("Styles", required: false)
</head>
<body data-theme="light" class="font-nunito">
    <div id="wrapper" class="theme-cyan">
        <!-- Page Loader -->
        <div class="page-loader-wrapper">
            <div class="loader">
                <div class="m-t-30">
                    <img src="~/boostionic/html/dist/assets/images/logo-icon.svg" width="48" height="48" alt="Iconic">
                </div>
                <p>Please wait...</p>
            </div>
        </div>

        @await Component.InvokeAsync("Sidebar", new { viewName = "TopBar" })
        @await Component.InvokeAsync("Sidebar", new { viewName = "LeftSideBar" })
        @await Component.InvokeAsync("Sidebar", new { viewName = "RightSideBar" })

        <div id="main-content">
            @RenderBody()
        </div>
    </div>
    <!-- Javascript -->
    <script src="~/boostionic/html/dist/assets/bundles/libscripts.bundle.js"></script>
    <script src="~/boostionic/html/dist/assets/bundles/vendorscripts.bundle.js"></script>
    <script src="~/js/logout.js"></script>

    <!-- page vendor js file -->
    <script src="~/boostionic/html/dist/assets/vendor/toastr/toastr.js"></script>
    <script src="~/boostionic/html/dist/assets/bundles/c3.bundle.js"></script>

    <!-- page js file -->
    <script src="~/boostionic/html/dist/assets/bundles/mainscripts.bundle.js"></script>
    <script src="~/boostionic/html/dist/../js/index.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>